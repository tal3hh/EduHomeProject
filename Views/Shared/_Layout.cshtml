                                            <ul>
                                                <li><a asp-action="BlogSideBar" asp-controller="Blog" >blog righ sidebar</a></li>
                                            </ul>
                                        </li>
                                        <li><a asp-action="Index" asp-controller="Contact">Contact</a></li>
                                    </ul>
                                </nav>
                            </div>

                            <!-- Main Menu End -->
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="mobile-menu hidden-lg hidden-md hidden-sm"></div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Header Area End -->
    <div>
        @RenderBody()
    </div>

   
