    <header class="top">
        <div class="header-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-sm-8 col-xs-12">
                     <div class="header-top-left">
                            <p>HAVE ANY QUESTION ? +55 856 23 93 </p>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="header-top-right text-right">
                            <ul>
                                @if (!User.Identity.IsAuthenticated)
                                {
                                    <li><a asp-action="Login" asp-controller="Account">login</a></li>
                                    <li><a asp-action="SingUp" asp-controller="Account">signup</a></li>
                                                
                                 }
                                @if (User.Identity.IsAuthenticated)
                                {
                                    <li><a asp-action="SingOut" asp-controller="Account">LogOut</a></li>
                                    if (User.IsInRole("Admin"))
                                    {
                                    <li><a asp-action="Index" asp-controller="AdminPanel" asp-area="Admin">AdminPanel</a></li>
                                    }
                                    
                                }

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
